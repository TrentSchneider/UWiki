<div class="container">
  <div class="collection">
    <div class="container">
      <a class='dropdown-trigger btn' href='#' data-target='dropdown1'>Choose a wiki: </a>
      <ul id='search' class='dropdown-content'>
        {{#each wikis}}
        <li><a href="/category/" + {{this.category}}> {{this.category}} </a></li>
        {{/each}}
      </ul>
    </div>
    <div id="uWikiList">
      <ul>
        {{#each wikis}}
        <li>
          <p>
            Category:
            {{this.category}}
          </p>
          <a href="#!" class="collection-item" data-id="{{this.id}}">
            <p>
              Title:
              {{this.title}}
            </p>
          </a>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>