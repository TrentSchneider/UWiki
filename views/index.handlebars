<div class="container">
  <div class="collection">
    <div class="container">
      <a class='dropdown-trigger btn' href='#' data-target='dropdown1'>Filter Category</a>
      <ul id='dropdown1' class='dropdown-content'>
        {{#each wikis}}
        <li><a href="/category/" + {{this.dataValues.category}}>{{this.dataValues.category}}</a></li>
        {{/each}}
      </ul>
    </div>
    <div id="uWikiList">
      <ul>
        {{#each wikis}}
        <li>
          <p>
            Category:
            {{this.dataValues.category}}
          </p>
          <a href="/view/{{this.dataValues.id}}" class="collection-item" data-id="{{this.dataValues.id}}">
            <p>
              Title:
              {{this.dataValues.title}}
            </p>
          </a>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>

<script type="text/javascript" src="js/index.js"></script>