<div class="container">
  <h2 class="AboutTitle">
    Add a Wiki
  </h2>
</div>
<div class="container">
  <div class="card">
    <form id="add-form">
      <div class="input-group">

        <label for="title">Title</label>
        <textarea id="title" class="materialize-textarea label-icon"></textarea>

        <br>
        <label for="category">Category</label>
        <textarea id="category" class="materialize-textarea label-icon"></textarea>

        <br>
        <label for="description">Description</label>
        <textarea id="description" type="description" name="description"
          class="materialize-textarea label-icon"></textarea>

        <br>
        <button type="submit" id="add" class="input-group-addon btn"> Add </button>
      </div>
    </form>
  </div>
</div>

<script type="text/javascript" src="js/createWiki.js"></script>